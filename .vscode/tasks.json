{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: cl.exe build active file",
			"command": "cl.exe",
			"args": [
				"/std:c++17",
				"/Wall",
				"/wd4820",
				"/wd5045",
				"/wd4464",
				"/wd4514",
				"/Zi",
				"/EHa",
				"/Fe:",
				"${fileDirname}\\${fileBasenameNoExtension}.exe",
				"${file}",
				"/MDd",
				"/I",
				"D:\\Workspace\\3rdparty\\vcpkg\\installed\\x86-windows\\include",
				"D:\\Workspace\\3rdparty\\vcpkg\\installed\\x86-windows\\lib\\glfw3dll.lib",
				"D:\\Workspace\\3rdparty\\vcpkg\\installed\\x86-windows\\lib\\glad.lib",
				"kernel32.lib",
				"user32.lib",
				"gdi32.lib",
				"winspool.lib",
				"shell32.lib",
				"ole32.lib",
				"oleaut32.lib",
				"uuid.lib",
				"comdlg32.lib",
				"advapi32.lib",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: cl.exe"
		}
	]
}